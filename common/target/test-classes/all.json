{
    "entry": {
        "jvm": "-Xms1G -Xmx1G",
        "environment": {
            "PATH": "/home/admin",
            "DATAX_HOME": "/home/admin"
        	}
    	},
       "data": {
        "column": {
            "number": {
                "bool": {
                    "0": false,
                    "!0": true
                }
            },
            "string": {
                "date": {
                    "timeFormat": "yyyy-MM-dd HH:mm:ss",
                    "timeZone": "GMT+8"
                },
                "bool": {
                    "true": true,
                    "!true": false
                },
                "bytes": {
                    "encoding": "utf-8"
                }
            },
            "date": {
                "string": {
                    "timeFormat": "yyyy-MM-dd HH:mm:ss",
                    "timeZone": "GMT+8"
                }
            },
            "bool": {
                "number": {
                    "true": 1,
                    "false": 0
                },
                "string": {
                    "true": "true",
                    "false": "false"
                }
            },
            "bytes": {
                "encoding": "utf-8"
            }
        }
    },
    "core": {
        "channel": {
            "class": "com.alibaba.datax.core.MemoryChannel",
            "speed": 1000000,
            "capacity": 128,
            "queue": {
                "class": "com.alibaba.datax.core.transport.channel.memory.DoubleQueue",
                "timeOut": 30
            }
        },
        "container": {
            "master": {
                "class": "com.alibaba.datax.core.container.MasterContainer"
            },
            "slave": {
                "class": "com.alibaba.datax.core.container.SlaveContainer"
            }
        },
        "metric": {
            "class": "com.alibaba.datax.core.metric.HTTPMetric",
            "service": "http://localhost/xxx",
            "badRecord": {
                "errorLimit": 0.1,
                "reportLimit": 10000
            }
        },
        "scheduler": {
            "class": "com.alibaba.datax.core.scheduler.standalone.StandAloneScheduler"
        },
        "plugin": {
            "exchanger": {
                "class": "com.alibaba.datax.core.plugin.BufferedRecordExchanger",
                "bufferSize": 32
            }
        }
    },
    "plugin": {
        "reader": {
            "mysqlreader": {
                "name": "fakereader",
                "class": "com.alibaba.datax.plugins.reader.fakereader.FakeReader",
                "description": "useScene: only for performance test. \n mechanism: Produce Record from memory. \n warn: Never use it in your real job.",
                "developer": "someBody,bug reported to : someBody@someSite"
            },
            "oraclereader": {
                "name": "oraclereader",
                "class": "com.alibaba.datax.plugins.reader.oraclereader.OracleReader",
                "description": "useScene: only for performance test. \n mechanism: Produce Record from memory. \n warn: Never use it in your real job.",
                "developer": "someBody,bug reported to : someBody@someSite"
            },
            "fakereader": {
                "name": "fakereader",
                "class": "com.alibaba.datax.core.faker.FakeReader",
                "description": "useScene: only for performance test. \n mechanism: Produce Record from memory. \n warn: Never use it in your real job.",
                "developer": "someBody,bug reported to : someBody@someSite"
            },
        },
        "writer": {
            "fakewriter": {
                        "name": "fakewriter",
                        "class": "com.alibaba.datax.core.faker.FakeWriter",
                        "description": "useScene: only for performance test. \n mechanism: Produce Record from memory. \n warn: Never use it in your real job.",
                        "developer": "someBody,bug reported to : someBody@someSite"
                    }
        },
        "transformer": {
            "groovyTranformer": {}
        },
    },
    "job": {
        "setting": {
            "speed": 10
        },
        "content": [
            {
                "reader": {
                    "name": "fakereader",
                    "parameter": {
                        "jdbcUrl": [
                            [
                                "jdbc:mysql://localhost:3305/db1",
                                "jdbc:mysql://localhost:3306/db1"
                            ],
                            [
                                "jdbc:mysql://localhost:3305/db2",
                                "jdbc:mysql://localhost:3306/db2"
                            ]
                        ],
                        "table": [
                            "bazhen_[0-15]",
                            "bazhen_[15-31]"
                        ]
                    }
                },
                "writer": {
                    "name": "fakewriter",
                    "paramter": {
                        "column": [
                            {
                                "type": "string",
                                "name": "id"
                            },
                            {
                                "type": "int",
                                "name": "age"
                            }
                        ],
                        "encode": "utf-8",
                        "hbase-conf": "/home/hbase/hbase-conf.xml"
                    }
                }
            }
        ]
    }
}