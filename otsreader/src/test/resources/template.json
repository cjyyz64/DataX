{
    "job": {
        "setting": {
            "speed": {
                "byte":10485760
            },
            "errorLimit":0.0
        },
        "content": [
            {
                "reader": {
                    "name": "otsreader",
                    "parameter": {
                        /* ----------- 必填 --------------*/
                        "endpoint":"",            
                        "accessId":"",            
                        "accessKey":"",           
                        "instanceName":"",
                        
                        // 导出数据表的表名   
                        "table":"",
                        
                        // 需要导出的列名，支持重复类和常量列，区分大小写
                        // 常量列：类型支持STRING，INT，DOUBLE，BOOL和BINARY
                        // 备注：BINARY需要通过Base64转换为对应的字符串传入插件
                        "column":[
                            {"name":"col1"},                          // 普通列
                            {"name":"col2"},                          // 普通列
                            {"name":"col3"},                          // 普通列
                            {"type":"STRING","value" : ""},           // 常量列(字符串)
                            {"type":"INT","value" : ""},              // 常量列(整形)
                            {"type":"DOUBLE","value" : ""},           // 常量列(浮点)
                            {"type":"BOOL","value" : ""},             // 常量列(布尔)
                            {"type":"BINARY","value" : "Base64(bin)"} // 常量列(二进制)
                        ], 
                        "range":{
                            // 导出数据的起始范围
                            // 支持INF_MIN, INF_MAX, STRING, INT
                            "begin":[
                                {"type":"INF_MIN", "value":""},
                                {"type":"INF_MAX", "value":""},
                                {"type":"STRING", "value":"hello"},
                                {"type":"INT", "value":"2999"},
                            ],
                            // 导出数据的结束范围
                            // 支持INF_MIN, INF_MAX, STRING, INT
                            "end":[
                                {"type":"INF_MAX", "value":""},
                                {"type":"INF_MIN", "value":""},
                                {"type":"STRING", "value":"hello"},
                                {"type":"INT", "value":"2999"},
                            ],
                            // 用户指定的切分点，如果指定了切分点，Job将按照begin、end和split进行Task的切分，
                            // 切分的列只能是Partition Key（ParimaryKey的第一列）
                            // 支持INF_MIN, INF_MAX, STRING, INT
                            "split":[
                                {"type":"INF_MIN", "value":""},
                                {"type":"STRING", "value":"1"},
                                {"type":"STRING", "value":"2"},
                                {"type":"STRING", "value":"3"},
                                {"type":"STRING", "value":"4"},
                                {"type":"STRING", "value":"5"},
                                {"type":"INF_MAX", "value":""}
                            ]
                        },
                        /* ----------- 选填 --------------*/
                        // 出现异常时，可以重复的次数
                        "maxRetryTime":12,
                        
                        // 每次重试的时候，Sleep的时间，单位为毫秒
                        "retrySleepInMillionSecond":100
                    }
                },
                "writer": {}
            }
        ]
    }
}