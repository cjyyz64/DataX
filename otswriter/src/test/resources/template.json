{
    "job": {
        "setting": {
            "speed": {
                "byte":10485760
            },
            "errorLimit":0.0
        },
        "content": [
            {
                "reader": {},
                "writer": {
                    "name": "otswriter",
                    "parameter": {
                        "endpoint":"", 
                        "accessId":"",
                        "accessKey":"",
                        "instanceName":"",
                        // 导出数据表的表名
                        "table":"",
                        
                        // Writer支持不同类型之间进行相互转换，转换规则请参见Datax文档
                        // 如下类型转换不支持:
                        // ================================
                        //    int    -> binary
                        //    double -> bool, binary
                        //    bool   -> binary
                        //    bytes  -> int, double, bool
                        // ================================
                        
                        // 需要导入的PK列名，区分大小写
                        // 类型支持：STRING，INT
                        // 1. 支持类型转换，注意类型转换时的精度丢失
                        // 2. 顺序不要求和表的Meta一致
                        "primaryKey" : [
                            {"name":"pk1", "type":"string"},
                            {"name":"pk2", "type":"int"}
                        ],
                        
                        // 需要导入的列名，区分大小写
                        // 类型支持STRING，INT，DOUBLE，BOOL和BINARY
                        // 备注：BINARY需要通过Base64转换为对应的字符串传入插件
                        // 1. 支持类型转换，注意类型转换时的精度丢失
                        "column" : [
                            {"name":"col2", "type":"INT"},
                            {"name":"col3", "type":"STRING"},
                            {"name":"col4", "type":"STRING"},
                            {"name":"col5", "type":"BINARY"},
                            {"name":"col6", "type":"DOUBLE"}
                        ], 
                        
                        // 写入OTS的方式
                        // PutRow : 等同于OTS API中PutRow操作，检查条件是ignore
                        // UpdateRow : 等同于OTS API中UpdateRow操作，检查条件是ignore
                        "writeMode" : "PutRow"
                    }
                }
            }
        ]
    }
}